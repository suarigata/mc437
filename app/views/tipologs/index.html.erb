<h1>Listing tipologs</h1>

<table>
  <tr>
    <th>Nome</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tipologs.each do |tipolog| %>
  <tr>
    <td><%= tipolog.nome %></td>
    <td><%= link_to 'Show', tipolog %></td>
    <td><%= link_to 'Edit', edit_tipolog_path(tipolog) %></td>
    <td><%= link_to 'Destroy', tipolog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tipolog', new_tipolog_path %>
